 
{%spaceless%}
{% for desktop in bspwm.desktops%}
{% if desktop.focused %}
{{ (' ' ~ (desktop.name) ~ ' ')|color_fg(backgroundColor)|color_bg(focusedDesktopColor) }}
{% elseif desktop.occupied %}
{{ (' ' ~ (desktop.name) ~ ' ')|color_fg(focusedDesktopColor) }}
{% else %}
{{ (' ' ~ (desktop.name) ~ ' ')|color_fg(occupiedDesktopColor) }}
{% endif %}
{%endfor%}
{%endspaceless%}
